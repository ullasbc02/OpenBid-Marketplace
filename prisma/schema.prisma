generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       BigInt @id @default(autoincrement())
  username String
  email    String @unique(map: "unique_email")
  password String
}
